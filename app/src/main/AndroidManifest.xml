<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nappy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.AppClient"
        android:allowBackup="true"
        android:icon="@drawable/app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="landscape"
            android:name=".ui.activity.MainActivity">

        </activity>
        <activity
            android:screenOrientation="landscape"
            android:name=".ui.activity.WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="landscape"
            android:name=".ui.activity.LoginActivity" >

        </activity>
        <activity
            android:screenOrientation="landscape"
            android:name=".test.TestActivity">

        </activity>
        <activity
            android:screenOrientation="landscape"
            android:name=".ui.activity.QueryResultsActivity" />
        <activity
            android:screenOrientation="landscape"
            android:name=".ui.activity.IllegalImageActivity" />

        <activity
            android:screenOrientation="landscape"
            android:name=".ui.activity.ImageReadActivity">

        </activity>
    </application>

</manifest>